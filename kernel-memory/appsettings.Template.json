{
    "KernelMemory": {
      "Service": {
        "RunWebService": true,
        "RunHandlers": true,
        "OpenApiEnabled": true
      },
      "ContentStorageType": "SimpleFileStorage",
      "TextGeneratorType": "OpenAI",
      "ServiceAuthorization": {
        "Enabled": false,
        "AuthenticationType": "APIKey",
        "HttpHeaderName": "Authorization",
        "AccessKey1": "",
        "AccessKey2": ""
      },
      "DataIngestion": {
        "OrchestrationType": "InProcess",
        "DistributedOrchestration": {
          "QueueType": ""
        },
        "EmbeddingGenerationEnabled": true,
        "EmbeddingGeneratorTypes": [
          "OpenAI"
        ],
        "MemoryDbTypes": [
          "Redis"
        ],
        "ImageOcrType": "None",
        "TextPartitioning": {
          "MaxTokensPerParagraph": 1000,
          "MaxTokensPerLine": 300,
          "OverlappingTokens": 100
        },
        "DefaultSteps": [
          "extract",
          "partition",
          "gen_embeddings",
          "save_records"
        ]
      },
      "Retrieval": {
        "MemoryDbType": "Redis",
        "EmbeddingGeneratorType": "OpenAI",
        "SearchClient": {
          "MaxAskPromptSize": -1,
          "MaxMatchesCount": 100,
          "AnswerTokens": 300,
          "EmptyAnswer": "INFO NOT FOUND"
        }
      },
      "Services": {
        "SimpleFileStorage": {
          "Directory": "/tmp"
        },
        "OpenAI": {
          "TextModel": "gpt-3.5-turbo-16k",
          "EmbeddingModel": "text-embedding-ada-002",
          "APIKey": "APIKey",
          "OrgId": "",
          "MaxRetries": 10
        },
        "Redis": {
          "Tags": {
            "user": ",",
            "email": "|"
          },
          "ConnectionString": "redis-km:6379"
        }
      }
    },
    "Logging": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "AllowedHosts": "*"
  }